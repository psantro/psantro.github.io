{
    "[css]": {
        "editor.rulers": [
            100
        ],
        "editor.wordWrap": "on",
        "editor.wordWrapColumn": 100
    },
    "[html]": {
        "editor.rulers": [
            100
        ],
        "editor.wordWrap": "on",
        "editor.wordWrapColumn": 100
    },
    "[javascript]": {
        "editor.rulers": [
            100
        ],
        "editor.wordWrap": "on",
        "editor.wordWrapColumn": 100
    },
    "editor.detectIndentation": false,
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "editor.wordWrap": "off",
    "files.autoGuessEncoding": false,
    "files.encoding": "utf8",
    "files.eol": "\n",
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true
}
